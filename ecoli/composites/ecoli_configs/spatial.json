{
    "spatial_environment" : true,
    "spatial_environment_config": {
        "multibody": {
            "bounds": [
                "!units[10 micrometer]",
                "!units[10 micrometer]"
            ]
        },
        "diffusion": {
            "molecules": ["GLC[p]"],
            "n_bins": [10, 10],
            "size": [
                "!units[10 micrometer]",
                "!units[10 micrometer]"
            ],
            "depth": "!units[3000.0 micrometer]",
            "diffusion": "!units[1e-2 micrometer ** 2 / second]",
            "gradient": {
                "type": "uniform",
                "molecules": {
                    "GLC[p]": "!units[1.0 millimolar]"
                }
            }
        }
    },
    "add_processes": [
      "local_field",
      "ecoli-shape"
    ],
    "topology": {
      "local_field": {
        "exchanges": ["environment", "exchange"],
        "location": ["boundary", "location"],
        "fields": ["..", "..", "fields"],
        "dimensions": ["..", "..", "dimensions"]
      },
      "ecoli-shape": {
        "cell_global": ["boundary"],
        "periplasm_global": ["periplasm", "global"],
        "listener_cell_mass": ["listeners", "mass", "cell_mass"],
        "listener_cell_volume": ["listeners", "mass", "volume"]
      }
    },
    "process_configs": {
      "local_field": {
        "n_bins": [10, 10]
      },
      "ecoli-shape": {}
    },
    "flow": {
      "ecoli-shape": [["ecoli-mass-listener"]]
    }
}
