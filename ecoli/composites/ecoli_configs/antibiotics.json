{
    "inherit_from": ["spatial.json", "cloud.json"],
    "add_processes": [
        "concentrations_deriver"
    ],
    "total_time": 12000,
    "spatial_environment_config": {
        "multibody": {
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ]
        },
        "reaction_diffusion": {
            "molecules": ["GLC[p]", "nitrocefin", "cephaloridine", "tetracycline", "ampicillin"],
            "n_bins": [10, 10],
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ],
            "depth": "!units[3000.0 micrometer]",
            "diffusion": "!units[1e-2 micrometer ** 2 / second]",
            "gradient": {
                "type": "uniform",
                "molecules": {
                    "GLC[p]": 1.0,
                    "nitrocefin": 0,
                    "cephaloridine": 0,
                    "tetracycline": 0,
                    "ampicillin": 0
                }
            }
        },
        "field_timeline": {
            "bins": [10, 10]
        },
        "field_timeline_topology": {
            "global": ["global"],
            "fields": ["fields"]
        }
    },
    "topology": {
        "concentrations_deriver": {
            "counts": ["bulk"],
            "concentrations": ["periplasm", "concentrations"],
            "volume": ["periplasm", "global", "volume"]
        }
    },
    "process_configs": {
        "concentrations_deriver": {
            "variables": ["EG10040-MONOMER[p]", "TRANS-CPLX-201[m]"]
        },
        "death": {
            "to_remove": [
                "ecoli-tf-binding_evolver",
                "ecoli-transcript-initiation_evolver",
                "ecoli-transcript-elongation_evolver",
                "ecoli-rna-degradation_evolver",
                "ecoli-polypeptide-initiation_evolver",
                "ecoli-polypeptide-elongation_evolver",
                "ecoli-complexation_evolver",
                "ecoli-two-component-system_evolver",
                "ecoli-equilibrium_evolver",
                "ecoli-protein-degradation_evolver",
                "ecoli-chromosome-replication_evolver",
                "ecoli-tf-binding_requester",
                "ecoli-transcript-initiation_requester",
                "ecoli-transcript-elongation_requester",
                "ecoli-rna-degradation_requester",
                "ecoli-polypeptide-initiation_requester",
                "ecoli-polypeptide-elongation_requester",
                "ecoli-complexation_requester",
                "ecoli-two-component-system_requester",
                "ecoli-equilibrium_requester",
                "ecoli-protein-degradation_requester",
                "ecoli-chromosome-replication_requester",
                "monomer_counts_listener",
                "mRNA_counts_listener",
                "allocator",
                "ecoli-chromosome-structure",
                "ecoli-metabolism",
                "death",
                "antibiotic-transport-steady-state",
                "permeability",
                "conc_to_counts"
            ]
        }
    },
    "flow": {
        "concentrations_deriver": [["ecoli-shape"]]
    },
    "engine_process_reports": [
        ["bioscrape_deltas"],
        ["periplasm"],
        ["cytoplasm"],
        ["kinetic_parameters"]
    ]
}
