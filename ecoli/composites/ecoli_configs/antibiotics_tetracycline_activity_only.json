{
    "inherit_from": ["spatial.json", "cloud.json"],
    "add_processes": [
        "chemostat",
        "tetracycline-ribosome-equilibrium"
    ],
    "total_time": 100,
    "spatial_environment_config": {
        "multibody": {
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ]
        },
        "diffusion": {
            "molecules": ["GLC[p]"],
            "n_bins": [10, 10],
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ],
            "depth": "!units[3000.0 micrometer]",
            "diffusion": "!units[1e-2 micrometer ** 2 / second]",
            "gradient": {
                "type": "uniform",
                "molecules": {
                    "GLC[p]": 1.0
                }
            }
        }
    },
    "topology": {
        "chemostat": {
            "tetracycline": ["cytoplasm", "concentrations", "tetracycline"]
        },
        "tetracycline-ribosome-equilibrium": {
            "tetracycline": ["cytoplasm", "concentrations", "tetracycline"],
            "30s-free": ["bulk", "CPLX0-3953[c]"],
