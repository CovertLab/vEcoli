    },
    "process_configs": {
        "local_field": {
            "n_bins": [10, 10],
            "bounds": [
                "!units[50 micrometer]",
                "!units[50 micrometer]"
            ]
        },
        "chemostat": {
            "targets": {
                "tetracycline": "!units[0.01 millimolar]"
            },
            "delay": 10
        },
        "tetracycline-ribosome-equilibrium": {},
        "ecoli-mass-listener": {
            "add_tetracycline_masses": true
        }
    },
    "flow": {
        "concentrations_deriver": [["ecoli-shape"]],
        "ecoli-polypeptide-initiation_requester": [["tetracycline-ribosome-equilibrium"]]
    },
    "engine_process_reports": [
        ["periplasm"],
        ["cytoplasm"],
        ["listeners", "mass"],
        ["boundary"]
    ]
